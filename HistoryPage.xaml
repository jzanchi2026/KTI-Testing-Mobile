<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp2.HistoryPage"
             Title="HistoryPage">
    
    
    
    
    <Grid RowDefinitions= "100, *, Auto, 75"
  ColumnDefinitions="0.25*, 0.25*, 0.25*, 0.25*"
  Padding="10"
  ColumnSpacing="10">


        <Button Text="Profile"
        Grid.Row="3"
        Grid.Column="0"
        Clicked="GoToProfilePage"
        
        />

        <Button Text="History"
        Grid.Row="3"
        Grid.Column="1"
        Clicked="GoToHistoryPage"
        />

        <Button Text="Cart"
        Grid.Row="3"
        Grid.Column="2"
        Clicked="GoToCartPage"
        />

        <Button Text="Settings"
        Grid.Row="3"
        Grid.Column="3"
        Clicked="GoToSettingPage"
        />

    </Grid>
</ContentPage>